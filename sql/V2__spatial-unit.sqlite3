
CREATE TABLE residence_claim (
       jwtRowId INTEGER,
       issuerDid TEXT, -- DID of the issuer; did:ethr are 52 chars
       ownedByDid TEXT, -- DID of the owning agent; did:ethr are 52 chars
       -- see https://schema.org/GeoShape basically contents of a WKT Polygon
       polygon TEXT,
       -- all WGS 84 (lat/long)
       westlon REAL,
       minlat REAL,
       eastlon REAL,
       maxlat REAL
);
